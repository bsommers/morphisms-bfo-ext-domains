CREATE CONSTRAINT IF NOT EXISTS FOR (n:Equipment) REQUIRE n.is_active IS NOT NULL;
CREATE CONSTRAINT IF NOT EXISTS FOR (n:Equipment) REQUIRE n.temperature IS NOT NULL;
