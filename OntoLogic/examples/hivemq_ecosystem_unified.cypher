CREATE CONSTRAINT IF NOT EXISTS FOR (n:MQTTBroker) REQUIRE n.active_connections IS NOT NULL;
CREATE CONSTRAINT IF NOT EXISTS FOR (n:SoftwareComponent) REQUIRE n.is_healthy IS NOT NULL;
CREATE CONSTRAINT IF NOT EXISTS FOR (n:SecurityArtifact) REQUIRE n.encryption_enabled IS NOT NULL;
CREATE CONSTRAINT IF NOT EXISTS FOR (n:Contract) REQUIRE n.contract_value IS NOT NULL;
CREATE CONSTRAINT IF NOT EXISTS FOR (n:DigitalCertificate) REQUIRE n.is_valid IS NOT NULL;
